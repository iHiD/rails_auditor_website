=form_for([@audit.project, @audit]) do |f|
    -if @audit.errors.any?
        #error_explanation
            %h2
                =pluralize(@project.errors.count, "error")
                prohibited this project from being saved:
            %ul
                -@project.errors.full_messages.each do |msg|
                    %li= msg

    .field
        =f.label :github_repository
        %br
        https://github.com/
        =f.text_field :github_repository

    .field
        =f.label :github_branch
        %br
        =f.text_field :github_branch
        
    .actions
        =f.submit "Audit my project"